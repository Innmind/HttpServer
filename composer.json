{
    "name": "innmind/http-server",
    "type": "library",
    "description": "Http server",
    "keywords": ["http", "server"],
    "homepage": "http://github.com/Innmind/HttpServer",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpServer/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/http": "^5.3.1|~6.0",
        "innmind/operating-system": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\HttpServer\\": "src/"
