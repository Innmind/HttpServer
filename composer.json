{
    "name": "innmind/http-server",
    "type": "library",
    "description": "Http server",
    "keywords": ["http", "server"],
    "homepage": "http://github.com/Innmind/HttpServer",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/HttpServer/issues"
    },
    "require": {
        "php": "~7.2"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\HttpServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\HttpServer\\": "tests/",
            "Fixture\\Innmind\\HttpServer\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "giorgiosironi/eris": "^0.9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
